# API Configuration
# URL base de la API de Star App 360
VITE_API_BASE_URL=https://startapp360.com/api/v1

# URL del proxy (solo para desarrollo local con proxy)
VITE_API_PROXY_URL=/api/proxy

# En desarrollo local, puedes usar el proxy para evitar CORS
# En producci√≥n (Vercel), esto debe ser 'false' para usar la API directa
VITE_USE_PROXY=false

# Authentication Storage Keys
VITE_JWT_STORAGE_KEY=auth_token
VITE_USERNAME_STORAGE_KEY=auth_username
VITE_PASSWORD_STORAGE_KEY=auth_password

# App Information
VITE_APP_NAME=Start App - Agency 360
VITE_APP_VERSION=1.0.0

# Debug mode (set to 'true' for development)
VITE_DEBUG=false
